{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",

    "allowJs": true,
    "checkJs": true,

    "noEmit": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,

    "strict": true,

    "skipLibCheck": true,
    "resolveJsonModule": true
  }
}
